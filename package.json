{
  "name": "data-warehouse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --fix",
    "lint:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "lint:ci:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --quiet",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --fix",
    "format": "pretty-quick --staged",
    "husky": "npm run format && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky"
    }
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "next": "10.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^5.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.3"
  }
}
